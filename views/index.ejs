<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
</head>
<body>
    <h1>You Searched for Stuff: <%= data.userQuery %></h1>
    <% if(data.loggedIn){ %>
        <h2>You are logged in as : <%= data.username %></h2>
    <% } %>
    <ul>
        <!-- <li><%= data.searchResults[0] %></li>
        <li>dummy data</li>
        <li>dummy data</li> -->
        <% data.searchResults.forEach(result=>{ %>
            <li> <%= result %> </li>
        <% }); %>
    </ul>
</body>
</html>